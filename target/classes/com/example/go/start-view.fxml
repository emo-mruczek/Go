<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.go.MainScreenFacade">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="192.0" layoutY="45.0" prefHeight="98.0" prefWidth="216.0" text="Go" textAlignment="CENTER">
         <font>
            <Font name="Bauhaus 93" size="96.0" />
         </font>
      </Label>
      <ButtonBar layoutX="166.0" layoutY="215.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="40.0" prefWidth="241.0">
         <buttons>
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#smallClicked" prefHeight="26.0" prefWidth="98.0" text="9x9" />
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#mediumClicked" prefHeight="5.0" text="13x13" textAlignment="CENTER" />
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#largeClicked" text="19x19" />
         </buttons>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </ButtonBar>
      <Label layoutX="252.0" layoutY="172.0" text="Quick hotseat">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <ButtonBar layoutX="122.0" layoutY="318.0" prefHeight="40.0" prefWidth="355.0">
         <buttons>
            <Button mnemonicParsing="false" onAction="#recapClicked" text="Previous games" />
            <Button mnemonicParsing="false" onAction="#onlineClicked" text="Online game" />
            <Button mnemonicParsing="false" onAction="#botClicked" text="No friends? :((" />
         </buttons>
      </ButtonBar>
      <Label layoutX="254.0" layoutY="281.0" text="Game modes">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
